        <entry>
          <string>Code</string>
          <string>module downcounter(
    input rst, c1khz,
    input count, load, 
    input [3:0] pm10,
    input [3:0] pm1,
    input [3:0] ps10,
    input [3:0] ps1,
    output reg [3:0] m10,
    output reg [3:0] m1,
    output reg [3:0] s10,
    output reg [3:0] s1,
    output zero);
    
    // write your code here

    assign zero = (s1 == 0 &amp;&amp; s10 == 0 &amp;&amp; m1 == 0 &amp;&amp; m10 == 0);
    always @(posedge count, negedge rst, posedge load)begin
        if(~rst)begin m10 &lt;= pm10; m1 &lt;= pm1; s10 &lt;= ps10; s1 &lt;= ps1;end
        else if(load) begin m10 &lt;= pm10; m1 &lt;= pm1; s10 &lt;= ps10; s1 &lt;= ps1;end
        else
	    begin
		if(s1 != 0)begin s1 &lt;= s1-1;end
		else if(s1 == 0 &amp;&amp; s10 != 0)begin s10 &lt;= s10 -1; s1 &lt;= 9;end
		else if(s1 == 0 &amp;&amp; s10 == 0 &amp;&amp; m1 != 0) begin s1 &lt;= 9; s10 &lt;= 5; m1 &lt;= m1-1;end
                if(s1 == 0 &amp;&amp; s10 == 0 &amp;&amp; m1 == 0 &amp;&amp; m10 != 0) begin s1 &lt;= 9; s10 &lt;= 5; m1 &lt;= 9; m10 &lt;= m10-1;end
	    end
    end

endmodule</string>
        </entry>
