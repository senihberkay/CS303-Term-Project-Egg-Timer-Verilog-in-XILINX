        <entry>
          <string>Code</string>
          <string>module controller(
    input clk,
    input rst,
    input beginn, 
    input reset,
    input zero,
    input endd,
    output load,
    output start,
    output countdown
    );

    // write your code here
    reg [1:0] counterdown;
    assign start = counterdown;
    assign load = reset;
    assign countdown = endd;

    always @(posedge beginn, posedge clk, posedge reset)begin
	if(beginn) counterdown = beginn;
	else if(endd) counterdown = endd;
	else if(reset) counterdown = 1&apos;b0; 
	else counterdown = 1&apos;b0;
	end

endmodule</string>
